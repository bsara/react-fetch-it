{
  "name": "react-fetch-it",
  "version": "1.0.0-rc.1",
  "description": "React hooks for interacting with the fetch API which includes the ability to add middleware to intercept, modify, cancel, cache, or analyze fetch calls.",
  "authors": [
    "Brandon Sara (https://bsara.dev)"
  ],
  "license": "ISC",
  "homepage": "https://github.com/bsara/react-fetch-it#readme",
  "repository": "bsara/react-fetch-it",
  "bugs": {
    "url": "https://github.com/bsara/react-fetch-it/issues"
  },
  "keywords": [
    "use-fetch",
    "fetch",
    "react",
    "hook",
    "react-hook",
    "react-use-fetch",
    "react-fetch",
    "react-fetch-hook",
    "http",
    "request"
  ],
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "README.md"
  ],
  "type": "module",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./MiddlewareProvider": {
      "types": "./middleware/FetchItMiddlewareProvider.d.ts",
      "default": "./middleware/FetchItMiddlewareProvider.js"
    },
    "./middleware/default-options": {
      "types": "./middleware/default-options-middleware.d.ts",
      "default": "./middleware/default-options-middleware.js"
    },
    "./middleware/simple-cache": {
      "types": "./middleware/simple-cache-middleware.d.ts",
      "default": "./middleware/simple-cache-middleware.js"
    },
    "./middleware/standard-options": {
      "types": "./middleware/standard-options-middleware.d.ts",
      "default": "./middleware/standard-options-middleware.js"
    }
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "dependencies": {
    "react-use": "^17.5.1"
  },
  "devDependencies": {
    "react": "^18.3.1"
  }
}
